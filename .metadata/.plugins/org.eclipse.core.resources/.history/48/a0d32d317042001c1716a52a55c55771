class Solution {
	  public static String camelCase(String input) {
	
		  String out = "";
	
		 for(int i = 0 ; i< input.length(); i ++) {
			 char ch = input.charAt(i);
				if(ch>='a' && ch<='z') out+= ch;
				if(ch>='A' && ch<='Z') out+= " "+ch;	
		 }
		  			   
		  return out;
		  //  return input.replaceAll("([A-Z])", " $1");  //Esto solo  
	  }
	}
//-----------------------------------------
*/

		String reg = "AbAAcAAAdAAAAeAAAAA";
		System.out.println(reg.replaceAll("\\S", "X"));
		System.out.println(reg.replaceAll("A*", "X"));
		System.out.println(reg.replaceAll("A?", "X"));
		System.out.println(reg.replaceAll("A*?", "X"));		
		

	}	
}		
 
 